#include "exynos5430.dtsi"

/ {
	model = "Samsung UNIVERSAL5430 board based on EXYNOS5430";
	compatible = "samsung,UNIVERSAL5430", "samsung,exynos5430";

	chosen {
		bootargs = "console=ttySAC1,115200 vmalloc=512M";
	};

	fixed-rate-clocks {
		oscclk {
			compatible = "samsung,exynos5430-oscclk";
			clock-frequency = <24000000>;
		};
	};

	dwmmc0@15540000 {
		status = "okay";
		num-slots = <1>;
		broken-cd;
		bypass-smu;
		fixed_volt;
		clock-gate;
		enable-cclk-on-suspend;
		only_once_tune;
		supports-highspeed;
		supports-hs200-1-8v-mode;
		supports-ddr200-1-8v-mode;
		device-driver = <1>;
		supports-poweroff-notification;
		supports-ddr200-enhanced-strobe;
		error-retry;
		enable-cache-control;
		enable-no-sleep-cmd;
		qos_int_level = <200000>;
		extra_tuning;
		bypass-for-allpass;
		use-fine-tuning;
		use-enable-shift;
		ignore-phase = <(1 << 7 | 1 << 6)>;
		fifo-depth = <0x80>;
		card-detect-delay = <200>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 2 0>;
		samsung,dw-mshc-ddr-timing = <3 0 3 2>;
		samsung,dw-mshc-hs200-timing = <3 0 3 0>;
		samsung,dw-mshc-ddr200-timing = <1 0 2 0>;
		samsung,dw-mshc-ddr200-delay-line = <100>;
		clk_pin = "gpr0-0";
		clk_addr = "15690000.pinctrl";
		clk_val = <0x2>;
		clk_str_num = <6>;
		num-ref-clks = <8>;
		ciu_clkin = <25 50 50 100 200 100 200 400>;
		pinctrl-names = "default";
		pinctrl-0 = <&sd0_clk &sd0_cmd &sd0_rdqs &sd0_bus1 &sd0_bus4 &sd0_bus8>;
		gpios = <&gpf3 5 0>;

		slot@0 {
			reg = <0>;
			bus-width = <8>;
		};
	};

	dwmmc1@15550000 {
		status = "okay";
		num-slots = <1>;
		channel = <1>;
		fixed_volt;
		clock-gate;
		enable-cclk-on-suspend;
		caps-control;
		supports-highspeed;
		supports-sdr104-mode;
		keep-power-in-suspend;
		pm-ignore-notify;
		cd-type-external;
		cd-type = <2>;
		use-broken-voltage;
		use-fine-tuning;
		ignore-phase = <(1 << 7)>;
		fifo-depth = <0x40>;
		card-detect-delay = <200>;
		qos_int_level = <200000>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 2 1>;
		samsung,dw-mshc-ddr-timing = <3 0 3 2>;
		clk_pin = "gpr2-0";
		clk_addr = "15690000.pinctrl";
		clk_val = <0x2>;
		clk_str_num = <6>;
		num-ref-clks = <8>;
		ciu_clkin = <25 50 50 100 200 100 200 400>;
		tp_mon_depth = <3>;
						/* Byte/s,	 MIF clk, CPU clk, KFC clk*/
		tp_mon_table = <18000000 825000	2000000 1500000
						12000000 413000	0	0
						0	 0	0	0>;
		pinctrl-names = "default";
		//pinctrl-0 = <&sd1_clk &sd1_cmd &sd1_bus1 &sd1_bus4>;
		pinctrl-0 = <&sd1_clk &sd1_cmd &sd1_bus1 &sd1_bus4 &cfg_wlanen &cfg_wlanhostwake>; /* Set WLAN_EN configuration of power down mode */

		slot@0 {
			reg = <0>;
			bus-width = <4>;
		};
	};


	dwmmc2@15560000 {
		status = "disabled";
		num-slots = <1>;
		supports-highspeed;
		clock-gate;
		enable-cclk-on-suspend;
		caps-control;
		cd-type = <0>;
		fifo-depth = <0x40>;
		card-detect-delay = <200>;
		qos_int_level = <200000>;
		use-fine-tuning;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 2 1>;
		samsung,dw-mshc-ddr-timing = <3 0 3 2>;
		clk_pin = "gpr4-0";
		clk_addr = "15690000.pinctrl";
		clk_val = <0x2>;
		clk_str_num = <6>;
		num-ref-clks = <8>;
		ciu_clkin = <25 50 50 100 200 100 200 400>;
		pinctrl-names = "default", "output";
		pinctrl-0 = <&sd2_clk &sd2_cmd &sd2_cd &sd2_bus1 &sd2_bus4 &dwmmc2_cd_irq>;
		pinctrl-1 = <&sd2_clk_output &sd2_cmd_output &sd2_bus1 &sd2_bus4 &dwmmc2_cd_irq>;

		gpio-controller;
		#gpio-cells = <2>;
		cd-gpio = <&gpa2 4 0xf>;

		slot@0 {
			reg = <0>;
			bus-width = <4>;
			disable-wp;
		};
	};

	serial@11460000 {
		samsung,uart-wake-peer;
		irq_affinity = <4>;
		mif_qos_val = <633000>;
		cpu_qos_val = <800000>;
		qos_timeout = <1000000>;
	};

	pinctrl@10580000 {
		ok_sw: ok-sw {
			samsung,pins = "gpa0-3";
			samsung,pin-pud = <3>;
			samsung,pin-drv = <0>;
		};
		pmic_irq: pmic-irq {
			samsung,pins = "gpa0-7";
			samsung,pin-function = <0>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <3>;
		};

		chg_irq: chg-irq {
			samsung,pins = "gpa1-5";
			samsung,pin-function = <0>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <3>;
		};

		hall_irq: hall-irq {
			samsung,pins = "gpa0-1";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};

		mfd_irq: mfd-irq {
			samsung,pins = "gpa1-4";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};

		fuel_irq: fuel-irq {
			samsung,pins = "gpa1-5";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};

		dwmmc2_cd_irq: dwmmc2-cd-irq {
			samsung,pins = "gpa2-4";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <3>;
		};

		ssp_irq: ssp-irq {
			samsung,pins = "gpa3-3";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <3>;
		};
		ssp_irq2: ssp-irq2 {
			samsung,pins = "gpa3-4";
			samsung,pin-function = <0>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <3>;
		};

		wc_irq: wc-irq {
			samsung,pins = "gpa3-5";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};

		ssp_ap_int: ssp-ap-int {
			samsung,pins = "gpa3-7";
			samsung,pin-function = <0>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <3>;
		};
		hrm_int: hrm-int {
			samsung,pins = "gpa0-6";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <3>;
		};
	};
	pinctrl@14CC0000 {
		/* Warm reset information from AP */
		pm_wrsti: pm-wrsti {
			samsung,pins = "gpf5-4";
			samsung,pin-con-pdn = <3>;
		};
	};
	pinctrl@14CC0000 {
		/* touchkey i2c*/
		cfg_i2c: cfg-i2c {
			samsung,pins = "gpb0-3", "gpb0-2";
			samsung,pin-function = <3>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		cfg_gpio: cfg_gpio {
			samsung,pins = "gpb0-3", "gpb0-2";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
	};

	pinctrl@10580000 {
		/* touchkey interrupt*/
		cfg_irq: cfg-irq {
			samsung,pins = "gpa1-6";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		cfg_input: cfg-input {
			samsung,pins = "gpa1-6";
			samsung,pin-function = <0>;
			samsung,pin-pud = <1>;
			samsung,pin-drv = <0>;
		};
	};

	hsi2c@14E40000 {
		samsung,hs-mode;
		clock-frequency = <400000>;
		status = "okay";

		s2mps13_pmic@66 {
			compatible = "samsung,s2mps13-pmic";
			reg = <0x66>;
			interrupts = <7 0 0>;
			interrupt-parent = <&gpa0>;
			pinctrl-names = "default";
			pinctrl-0 = <&pmic_irq &pm_wrsti>;
			ap-buck-avp-en;
			sub-buck-avp-en;
			/* RTC: wtsr/smpl */
			wtsr_en = <1>; /* enable */
			smpl_en = <1>; /* enable */
			wtsr_timer_val = <3>;	/* 1000ms */
			smpl_timer_val = <4>;	/* 500ms */
			check_jigon = <0>; /* do not check jigon */
			/* RTC: If it's first boot, reset rtc to 1/1/2014 12:00:00(Wed) */
			init_time,sec = <0>;
			init_time,min = <0>;
			init_time,hour = <12>;
			init_time,mday = <1>;
			init_time,mon = <0>;
			init_time,year = <114>;
			init_time,wday = <3>;
		};
	};

	i2c@14E90000 {
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <400000>;
		status = "okay";
		pinctrl-names = "default","on_i2c","off_i2c";
		pinctrl-0 = <&i2c5_bus>;
		pinctrl-1 = <&cfg_i2c>;
		pinctrl-2 = <&cfg_gpio>;
		touchkey@20 {
			pinctrl-names = "on_irq","off_irq";
			pinctrl-0 = <&cfg_irq>;
			pinctrl-1 = <&cfg_input>;
			compatible = "cypress,cypress_touchkey";
			reg = <0x20>;
			interrupts = <6 0 0>;
			interrupt-parent = <&gpa1>;
			cypress,sda-gpio = <&gpb0 2 0>;
			cypress,scl-gpio = <&gpb0 3 0>;
			cypress,irq-gpio = <&gpa1 6 1>;
			cypress,ic_type = <20055>;
			cypress,fw_path = "cypress/cypress_slte.fw";
			cypress,boot_on_ldo;
			cypress,led_by_ldo;
			cypress,glove_mode_keep_status;
		};
	};

	i2c_8: i2c@14130000 {
                compatible = "samsung,exynos5430-fimc-i2c";
                reg = <0x14130000 0x1000>;
                interrupts = <0 166 0>; /* NON */
                #address-cells = <1>;
                #size-cells = <0>;
                pinctrl-names = "default";
                pinctrl-0 = <&fimc_is_i2c_0>;
                clocks = <&clock 4184>, <&clock 4184>; /* No clock gating */
                clock-names = "rate_i2c","gate_i2c";
                samsung,i2c-sda-delay = <100>;
                samsung,i2c-max-bus-freq = <400000>;
                status = "okay";

                fimc_is_i2c0@3d {
                        compatible = "samsung,fimc_is_i2c0";
                        reg = <0x3d>;
                };
        };


	i2c@14EA0000 {
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <400000>;
		status = "okay";

		max77804@66 {
			compatible = "maxim,max77804";
			reg = <0x66>;
			pinctrl-names = "default";
			pinctrl-0 = <&mfd_irq &wc_irq &vibetonz_pwm>;
			max77804,irq-gpio = <&gpa1 4 1>;
			max77804,irqf-trigger = "low_level";
			max77804,wakeup;

			regulators {
				safeout1: ESAFEOUT1 {
					regulator-compatible = "safeout1";
					regulator-name = "safeout1_range";
					regulator-min-microvolt = <3300000>;
					regulator-max-microvolt = <4900000>;
					regulator-boot-on;
				};

				safeout2: ESAFEOUT2 {
					regulator-compatible = "safeout2";
					regulator-name = "safeout2_range";
					regulator-min-microvolt = <3300000>;
					regulator-max-microvolt = <4900000>;
				};
			};
		};

		max77828@49 {
			status = "disabled";
			compatible = "maxim,max77828";
			reg = <0x49>;
			pinctrl-names = "default";
			pinctrl-0 = <&mfd_irq &vibetonz_pwm>;
			max77828,irq-gpio = <&gpa1 4 1>;
			max77828,wakeup;

			haptic {
				haptic,max_timeout = <10000>;
				haptic,duty = <34500>;
				haptic,period = <38240>;
				haptic,pwm_id = <0>;
				haptic,regulator_name = "VCC_3.0V_MOTOR_AP";
			};
		};

	};

	haptic {
		status = "okay";
		haptic,max_timeout = <10000>;
		haptic,duty = <28000>;
		haptic,period = <33500>;
		haptic,reg2 = <0x82>;
		haptic,pwm_id = <0>;
		haptic,regulator_name = "VCC_3.0V_MOTOR_AP";
	};

	torch {
		status = "okay";
		leds1 {
			ledname = "leds-sec1";
		/* MAX77804_FLASH_LED_1 */
			id = <0>;
		/* MAX77804_FLASH_TIME_187P5MS */
			timer = <2>;
		/* MAX77804_TIMER_MODE_MAX_TIMER */
			timer_mode = <1>;
		/* MAX77804_LED_CTRL_BY_FLASHSTB */
			cntrl_mode = <0>;
			brightness = <0x3D>;
		};
		leds2 {
			ledname = "torch-sec1";
		/* MAX77804_TORCH_LED_1 */
			id = <1>;
		/* MAX77804_LED_CTRL_BY_FLASHSTB */
			cntrl_mode = <0>;
			brightness = <0x06>;
		};
	};

	i2c@14E00000 {
		fimc_is_fan53555@60 {
			compatible = "samsung,fimc_is_fan53555";
			reg = <0x60>;
			comp_en = <&gpf1 7 0x01>;
			status = "okay";
		};
	};

	pinctrl@10580000 {
		disp_pcd: disp_pcd {
			samsung,pins = "gpa0-5";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <1>;
		};
	};

	pinctrl@14CC0000 {
		disp_errfg: disp_errfg {
			samsung,pins = "gpb0-4";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <1>;
		};
		disp_errfg_off: disp_errfg_off {
			samsung,pins = "gpb0-4";
			samsung,pin-function = <1>;
			samsung,pin-pud = <1>;
			samsung,pin-val = <0>;
		};
		disp_id: disp_id {
			samsung,pins = "gpf2-1";
			samsung,pin-function = <0>;
			samsung,pin-pud = <1>;
		};
	};

	/* disp_driver */
	decon_fb: decon_fb {
		oled-id-gpio = <&gpf2 1 0x0>;
		oled-pcd-gpio = <&gpa0 5 0xf>;
		oled-errfg-gpio = <&gpb0 4 0xf>;
		lcd_info = <&ea8064g_488 &ea8064g>;
		display,regulator-names = "VCC_1.8V_LCD_AP", "VCC_3.0V_LCD_AP";

		pinctrl-names = "default", "turnon_tes", "turnoff_tes", "errfg_off";
		pinctrl-0 = <&disp_id &disp_pcd &disp_errfg>;
		pinctrl-1 = <&disp_id &disp_pcd &disp_errfg &disp_teson>;
		pinctrl-2 = <&disp_id &disp_tesoff>;
		pinctrl-3 = <&disp_id &disp_pcd &disp_errfg_off &disp_teson>;

		mipi_dsi: mipi_dsi {
			gpios = <&gpv7 2 0x1>, <&gpf1 5 0x1>;

			/* Interface definition: 0 - DSIM_COMMAND,
						 1 - DSIM_VIDEO */
			e_interface = <0>;

			/* Pixel format : 0 - DSIM_CMD_3BPP,
					  1 - DSIM_CMD_8BPP,
					  2 - DSIM_CMD_12BPP,
					  3 - DSIM_CMD_16BPP,
					  4 - DSIM_VID_16BPP_565,
					  5 - DSIM_VID_18BPP_666PACKED,
					  6 - DSIM_18BPP_666LOOSELYPACKED,
					  7 - DSIM_24BPP_888 */
			e_pixel_format = <7>;

			/* main frame fifo auto flush at VSYNC pulse */
			auto_flush = <0>;
			eot_disable = <1>;
			auto_vertical_cnt = <0>;
			hse = <0>;
			hfp = <0>;
			hbp = <0>;
			hsa = <0>;

			/* num of data lane : 0 - DSIM_DATA_LANE_1,
					      1 - DSIM_DATA_LANE_2,
					      2 - DSIM_DATA_LANE_3,
					      3 - DSIM_DATA_LANE_4 */
			e_no_data_lane = <3>;
			/* byte clock divider : 0 - DSIM_PLL_OUT_DIV8,
						1 - DSIM_EXT_CLK_DIV8,
						2 - DSIM_EXT_CLK_BYPASS */
			e_byte_clk = <0>;
			/* Burst mode : 0 - DSIM_NON_BURST_SYNC_EVENT,
					2 - DSIM_NON_BURST_SYNC_PULSE,
					1 - DSIM_BURST
					4 - DSIM_NON_VIDEO_MODE */
			e_burst_mode = <1>;
			/* D-PHY PLL stable time spec :min = 200usec ~ max 400usec */
			pll_stable_time = <22200>;
			/* stop state holding counter after bta change count 0 ~ 0xfff */
			stop_holding_cnt = <10>;
			/* bta timeout 0 ~ 0xff */
			bta_timeout = <0xff>;
			/* lp rx timeout 0 ~ 0xffff */
			rx_timeout = <0xffff>;
		};
		ea8064g_488: ea8064g_488 {
			mode = <0>;
			resolution = <720 1280>;
			size = <58 104>;
			timing,refresh = <60>;
			timing,h-porch = <1 1 1>;
			timing,v-porch = <12 1 1>;
			timing,dsi-hs-clk = <488>;
			timing,dsi-escape-clk = <16>;
			timing,hsync-2h-cycle = <1>;
			type_of_ddi = <1>; /* 0:Samsung Mobile, 1:MAGNA, 2:Normal(Etc) */
		};
		ea8064g: ea8064g_hd {
			mode = <0>;
			resolution = <720 1280>;
			size = <58 104>;
			timing,refresh = <60>;
			timing,h-porch = <1 1 1>;
			timing,v-porch = <12 1 1>;
			timing,dsi-hs-clk = <448>;
			timing,dsi-escape-clk = <16>;
			timing,hsync-2h-cycle = <1>;
			type_of_ddi = <1>; /* 0:Samsung Mobile, 1:MAGNA, 2:Normal(Etc) */
		};
	};

	usb@15400000 {
		status = "okay";
		/* using gpio (1:use, 0:not use) */
		is_gpio = <0>;

		dwc3 {
			maximum-speed = "high-speed";
			dr_mode = "otg";
		};
	};

	usb@15510000 {
		/* using gpio (1:use, 0:not use) */
		is_gpio = <0>;
	};

	usb_notifier {
		compatible = "samsung,usb-notifier";
	};

	gpio_keys {
		compatible = "gpio-keys";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <100>;
		pinctrl-names = "default";
		pinctrl-0 = <&ok_sw>;
		button@1 {
			label = "gpio-keys: KEY_POWER";
			interrupts = <7 0 0>;
			interrupt-parent = <&gpa2>;
			linux,code = <116>;
			gpios = <&gpa2 7 0xf>;
			gpio-key,wakeup = <1>;
		};
		button@2 {
			label = "gpio-keys: KEY_VOLUMEDOWN";
			interrupts = <1 0 0>;
			interrupt-parent = <&gpa2>;
			linux,code = <114>;
			gpios = <&gpa2 1 0xf>;
		};
		button@3 {
			label = "gpio-keys: KEY_VOLUMEUP";
			interrupts = <0 0 0>;
			interrupt-parent = <&gpa2>;
			linux,code = <115>;
			gpios = <&gpa2 0 0xf>;
		};
		button@4 {
			label = "gpio-keys: KEY_HOMEPAGE";
			interrupts = <3 0 0>;
			interrupt-parent = <&gpa0>;
			linux,code = <172>;
			gpios = <&gpa0 3 0xf>;
			gpio-key,wakeup = <1>;
		};
	};

	sec_input_debug-keys {
		compatible = "sec_input_debug-keys";

		button@1 {
			label = "debug-keys: KEY_POWER";
			linux,code = <116>;
		};
		button@2 {
			label = "debug-keys: KEY_VOLUMEDOWN";
			linux,code = <114>;
			state = "true";
		};
		button@3 {
			label = "debug-keys: KEY_VOLUMEUP";
			linux,code = <115>;
