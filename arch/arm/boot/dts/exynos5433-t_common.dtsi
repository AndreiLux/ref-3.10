		        <&gpr3 5 0x1 /* PERST */>,
				<&gpf3 1 0x1 /* WLAN HOST WAKE GPIO_0 */>;
		reg = <0x156b0000 0x1000
			0x15680000 0x1000
			0x156f0000 0x2000
			0x105c0000 0x4
			0x0c000000 0x1000>;
		interrupts = <0 245 0>;
		clocks = <&clock 2176>,		/* gate_pcie */
			 <&clock 2177>;		/* gate_pcie_phy */
		clock-names = "gate_pcie", "gate_pcie_phy";
