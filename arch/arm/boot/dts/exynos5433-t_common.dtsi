#include "exynos5433.dtsi"

/ {

	model = "Samsung UNIVERSAL5433 board based on EXYNOS5433";
	compatible = "samsung,UNIVERSAL5433", "samsung,exynos5433";

	aliases {
		spi0 = &spi_0;
		spi1 = &spi_1;
		spi2 = &spi_2;
		spi3 = &spi_3;
		spi4 = &spi_4;
		spi5 = &spi_5;
		spi6 = &spi_6;
	};

	chosen {
		bootargs = "console=ttySAC1,115200 vmalloc=256M";
	};

	fixed-rate-clocks {
		oscclk {
			compatible = "samsung,exynos5433-oscclk";
			clock-frequency = <24000000>;
		};
	};

	dwmmc0@15540000 {
		status = "okay";
		num-slots = <1>;
		broken-cd;
		bypass-smu;
		clock-gate;
		enable-cclk-on-suspend;
		fixed_volt;
		only_once_tune;
		supports-highspeed;
		supports-hs200-1-8v-mode;
		supports-ddr200-1-8v-mode;
		supports-ddr200-enhanced-strobe;
		supports-poweroff-notification;
		device-driver = <1>;
		enable-cache-control;
		enable-no-sleep-cmd;
		use-fine-tuning;
		use-enable-shift;
		enable-ulp-mode;
		ignore-phase = <(1 << 7 | 1 << 6)>;
		extra_tuning;
		bypass-for-allpass;
		desc-size = <4>;
		fifo-depth = <0x80>;
		qos_int_level = <400000>;
		use-enable-shift;
		card-detect-delay = <200>;
		samsung,dw-mshc-ddr200-delay-line = <0x40>;
		samsung,dw-mshc-ddr200-tx-delay-line = <0x30>;
		samsung,dw-mshc-txdt-crc-timer-fastlimit = <0x34>;
		samsung,dw-mshc-txdt-crc-timer-initval = <0x38>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 2 0>;
		samsung,dw-mshc-sdr-hs-timing = <3 0 2 1>;
		samsung,dw-mshc-ddr-timing = <3 0 3 2>;
		samsung,dw-mshc-hs200-timing = <3 0 3 0>;
		samsung,dw-mshc-ddr200-timing = <1 0 3 0>;
		samsung,dw-mshc-ddr200-ulp-timing = <3 0 3 0>;
		clk_pin = "gpr0-0";
		clk_addr = "15690000.pinctrl";
		clk_val = <0x3>;
		clk_str_num = <6>;
		num-ref-clks = <10>;
		ciu_clkin = <25 50 50 100 200 100 200 400 400 200>;
		pinctrl-names = "default";
		pinctrl-0 = <&sd0_clk &sd0_cmd &sd0_rdqs &sd0_bus1 &sd0_bus4 &sd0_bus8>;
		gpios = <&gpd6 6 0>;
		slot@0 {
			reg = <0>;
			bus-width = <8>;
		};
	};

	dwmmc1@15550000 {
		status = "disabled";
		num-slots = <1>;
		fixed_volt;
		clock-gate;
		enable-cclk-on-suspend;
		only_once_tune;
		supports-highspeed;
		not-allow-single-dma;
		desc-size = <4>;
		fifo-depth = <0x40>;
		card-detect-delay = <200>;
		qos_int_level = <200000>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 2 1>;
		samsung,dw-mshc-ddr-timing = <3 0 3 2>;
		num-ref-clks = <8>;
		ciu_clkin = <25 50 50 100 200 100 200 400>;
		pinctrl-names = "default";
		pinctrl-0 = <&sd1_clk &sd1_cmd &sd1_bus1 &sd1_bus4>;

		slot@0 {
			reg = <0>;
			bus-width = <4>;
		};
	};

	dwmmc2@15560000 {
		status = "okay";
		num-slots = <1>;
		bypass-smu;
		supports-highspeed;
		supports-sdr104-mode;
		clock-gate;
		enable-cclk-on-suspend;
		fix-fmp-size-mismatch;
		not-allow-single-dma;
		desc-size = <4>;
		use-fine-tuning;
		bypass-for-allpass;
		cd-type = <0>;
		fifo-depth = <0x40>;
		card-detect-delay = <200>;
		qos_int_level = <400000>;
		ignore-phase = <(1 << 7)>;
		samsung,dw-mshc-ciu-div = <3>;
		samsung,dw-mshc-sdr-timing = <3 0 2 1>;
		samsung,dw-mshc-ddr-timing = <3 0 3 2>;
		clk_pin = "gpr4-0";
		clk_addr = "15690000.pinctrl";
		clk_val = <0x2>;
		clk_str_num = <6>;
		num-ref-clks = <8>;
		ciu_clkin = <25 50 50 100 200 100 200 400>;
		pinctrl-names = "default", "output";
		pinctrl-0 = <&sd2_clk &sd2_cmd &sd2_bus1 &sd2_bus4 &dwmmc2_cd_ext_irq &dwmmc2_vmmc_en_gpio>;
		pinctrl-1 = <&sd2_clk_output &sd2_cmd_output &sd2_bus1 &sd2_bus4 &dwmmc2_cd_ext_irq &dwmmc2_vmmc_en_gpio>;

		gpio-controller;
		#gpio-cells = <2>;
		cd-gpio = <&gpa2 4 0xf>;

		vmmc-en-gpio = <&gpf0 7 0x1>;

		slot@0 {
			reg = <0>;
			bus-width = <4>;
			disable-wp;
		};
	};

	pinctrl@11090000 {
		dwmmc2_vmmc_en_gpio: dwmmc2-vmmc-en-gpio {
			samsung,pins = "gpf0-7";
			samsung,pin-function = <0x1>;
			samsung,pin-pud = <0>;
		};
	};

	pinctrl@10580000 {
		ok_sw: ok-sw {
			samsung,pins = "gpa0-3";
			samsung,pin-pud = <3>;
			samsung,pin-drv = <0>;
		};
		dwmmc2_cd_ext_irq: dwmmc2_cd_ext_irq {
			samsung,pins = "gpa2-4";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		pmic_irq: pmic-irq {
			samsung,pins = "gpa0-7";
			samsung,pin-pud = <0>;
			samsung,pin-drv = <3>;
		};
		/* Warm reset information from AP */
		pm_wrsti: pm-wrsti {
			samsung,pins = "gpf5-4";
			samsung,pin-pud = <0>;
			samsung,pin-con-pdn = <3>;
		};

		gpa3_pull_up: gpa3_pull_up_for_smpl {
			samsung,pins = "gpa3-7";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <0>;
		};

		ssp_irq: ssp-irq {
			samsung,pins = "gpa3-3";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <1>;
			samsung,pin-drv = <3>;
		};

		ssp_mcu_req: ssp-mcu-req {
			samsung,pins = "gpf2-3";
			samsung,pin-function = <1>;
			samsung,pin-pud = <1>;
			samsung,pin-drv = <3>;
		};

		ssp_mcu_resp: ssp-mcu-resp {
			samsung,pins = "gpf2-2";
			samsung,pin-function = <0>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <3>;
		};

		ssp_host_wake: ssp-host-wake {
			samsung,pins = "gpa1-0";
			samsung,pin-function = <0>;
			samsung,pin-pud = <3>;
			samsung,pin-drv = <3>;
		};

		ssp_host_wake_irq: ssp-host-wake-irq {
			samsung,pins = "gpa1-0";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <1>;
			samsung,pin-drv = <3>;
		};
	};

	pinctrl@14CC0000 {
		ssp_gps_pwr_en: ssp-gps-pwr-en {
			samsung,pins = "gpg2-0";
			samsung,pin-function = <1>;
			samsung,pin-pud = <1>;
			samsung,pin-drv = <3>;
		};
	};

	gpio_keys {
		compatible = "gpio-keys";
		#address-cells = <1>;
		#size-cells = <0>;
		pinctrl-names = "default";
		pinctrl-0 = <&ok_sw>;
		button@1 {
			label = "gpio-keys: KEY_POWER";
			interrupts = <7 0 0>;
			interrupt-parent = <&gpa2>;
			linux,code = <116>;
			gpios = <&gpa2 7 0xf>;
			gpio-key,wakeup = <1>;
		};
		button@2 {
			label = "gpio-keys: KEY_VOLUMEDOWN";
			interrupts = <1 0 0>;
			interrupt-parent = <&gpa2>;
			linux,code = <114>;
			gpios = <&gpa2 1 0xf>;
		};
		button@3 {
			label = "gpio-keys: KEY_VOLUMEUP";
			interrupts = <0 0 0>;
			interrupt-parent = <&gpa2>;
			linux,code = <115>;
			gpios = <&gpa2 0 0xf>;
		};
		button@4 {
			label = "gpio-keys: KEY_HOMEPAGE";
			interrupts = <3 0 0>;
			interrupt-parent = <&gpa0>;
			linux,code = <172>;
			gpios = <&gpa0 3 0xf>;
			gpio-key,wakeup = <1>;
		};
	};

	pinctrl@10580000 {
		fuel_irq: fuel-irq {
			samsung,pins = "gpa1-5";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		wc_irq: wc-irq {
			samsung,pins = "gpa3-5";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
	};

	pinctrl@14CC0000 {
		/* touchkey i2c*/
		cfg_i2c: cfg-i2c {
			samsung,pins = "gpb0-3", "gpb0-2";
			samsung,pin-function = <4>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		cfg_gpio: cfg_gpio {
			samsung,pins = "gpb0-3", "gpb0-2";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
	};

	pinctrl@10580000 {
		/* touchkey interrupt*/
		cfg_irq: cfg-irq {
			samsung,pins = "gpa3-2";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		cfg_input: cfg-input {
			samsung,pins = "gpa3-2";
			samsung,pin-function = <0>;
			samsung,pin-pud = <1>;
			samsung,pin-drv = <0>;
		};
	};

	pinctrl@14CE0000 {
		hs_i2c5_bus: hs-i2c5-bus {
			samsung,pins = "gpj1-1", "gpj1-0";
			samsung,pin-function = <4>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
			samsung,pin-con-pdn = <2>;
			samsung,pin-pud-pdn = <1>;
		};
	};

	hsi2c@14D90000 {
		max77843@66 {
			compatible = "maxim,max77843";
			reg = <0x66>;
			pinctrl-names = "default";
			pinctrl-0 = <&fuel_irq &vibetonz_pwm &wc_irq>;
			max77843,irq-gpio = <&gpa1 5 1>;
			max77843,wakeup;

			regulators {
				safeout1: ESAFEOUT1 {
					regulator-compatible = "safeout1";
					regulator-name = "safeout1_range";
					regulator-min-microvolt = <3300000>;
					regulator-max-microvolt = <4900000>;
					regulator-boot-on;
				};

				safeout2: ESAFEOUT2 {
					regulator-compatible = "safeout2";
					regulator-name = "safeout2_range";
					regulator-min-microvolt = <3300000>;
					regulator-max-microvolt = <4900000>;
				};
			};
		};
	};

	haptic {
		status = "okay";
		haptic,max_timeout = <10000>;
		haptic,duty = <32448>;
		haptic,period = <33814>;
		haptic,reg2 = <0x82>;
		haptic,pwm_id = <0>;
		haptic,regulator_name = "VCC_3.0V_MOTOR_AP";
	};

	rgb {
		rgb-name = "led_w", "led_r", "led_g", "led_b";
	};

	hall {
		status = "okay";
		compatible = "hall";
		pinctrl-names = "default";
		pinctrl-0 = <&hall_irq>;
		hall,gpio_flip_cover = <&gpa0 1 0>;
	};

	pinctrl@14CC0000 {
		w1_gpio: w1-gpio {
			samsung,pins = "gpd2-5";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
	};

	w1_gpio_exynos {
		status = "okay";
		compatible = "w1_gpio_exynos";
		pinctrl-names = "default";
		pinctrl-0 = <&w1_gpio>;
		is_open_drain = <1>;
		slave_speed = <1>;
		register_addr = <0x14CC0120>;
		register_num = <5>;
		w1,gpio = <&gpd2 5 0>;
	};

	led_cover {
		compatible = "led_cover";
		ledcover_en,gpio = <&gpd4 1 0>;
	};

	usb_notifier {
		compatible = "samsung,usb-notifier";
	};

	muic {
		status = "disabled";
		muic,support-list = "MHL","OTG","Charging Cable","Jig USB On","Jig UART Off",
				    "Jig UART Off + VB","Jig UART On",
				    "TA","USB","CDP","Undefined Charging",
				    "Unofficial ID","Unofficial ID + TA","Unofficial ID + ANY TA",
				    "Unofficial ID + USB","Unofficial ID + CDP",
				    "Deskdock","Deskdock + VB",
				    "Smartdock","Smartdock + VB","Smartdock + TA","Smartdock + USB",
				    "Fuelgauge test","HMT";
		muic,afcmode-tx = /bits/ 8 <0x46>;
		muic,qc-hv = /bits/ 8 <0x9>;
	};

	hsi2c@14DA0000 {
		status = "okay";
		pinctrl-names = "default","on_i2c","off_i2c";
		pinctrl-0 = <&hs_i2c9_bus>;
		pinctrl-1 = <&cfg_i2c>;
		pinctrl-2 = <&cfg_gpio>;
		touchkey@20 {
			pinctrl-names = "on_irq","off_irq";
			pinctrl-0 = <&cfg_irq>;
			pinctrl-1 = <&cfg_input>;
			compatible = "cypress,cypress_touchkey";
			reg = <0x20>;
			interrupts = <2 0 0>;
			interrupt-parent = <&gpa3>;
			cypress,sda-gpio = <&gpb0 2 0>;
			cypress,scl-gpio = <&gpb0 3 0>;
			cypress,irq-gpio = <&gpa3 2 1>;
			cypress,ic-stabilizing-time = <150>;
			cypress,boot-on-ldo = "true";
		};
	};

	hsi2c@14E40000 {
		status = "okay";
		samsung,hs-mode;
		clock-frequency = <2500000>;

		s2mps13_pmic@66 {
			compatible = "samsung,s2mps13-pmic";
			reg = <0x66>;
			interrupts = <7 0 0>;
			interrupt-parent = <&gpa0>;
			gpios = <&gpa3 7 0>, <&gpf0 1 0x0>, <&gpg1 1 0x1>;
			smpl_warn_vth = <0x00>; /* 2.3V */
			smpl_warn_en = <1>; /* 1 : enable , 0 : disable*/
			g3d_en = <0>; /* 1 : enable , 0 : disable */
			dvs_en = <1>; /* 1 : enable , 0 : disable */
			/* RTC: wtsr/smpl */
			wtsr_en = <1>; /* enable */
			smpl_en = <1>; /* enable */
			wtsr_timer_val = <3>;	/* 1000ms */
			smpl_timer_val = <4>;	/* 500ms */
			check_jigon = <0>; /* do not check jigon */
			/* RTC: If it's first boot, reset rtc to 1/1/2014 12:00:00(Wed) */
			init_time,sec = <0>;
			init_time,min = <0>;
			init_time,hour = <12>;
			init_time,mday = <1>;
			init_time,mon = <0>;
			init_time,year = <114>;
			init_time,wday = <3>;
		};
	};

	pinctrl@10580000 {
		epen_irq: epen-irq {
			samsung,pins = "gpf4-3", "gpf4-4";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		epen_input: epen-input {
			samsung,pins = "gpf4-3", "gpf4-4";
			samsung,pin-function = <0>;
			samsung,pin-pud = <1>;
			samsung,pin-drv = <0>;
		};
		epen_sense: epen-sense {
			samsung,pins = "gpa1-6";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		epen_ldo: epen-ldo {
			samsung,pins = "gpf1-6";
			samsung,pin-function = <1>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
		hall_irq: hall-irq {
			samsung,pins = "gpa0-1";
			samsung,pin-function = <0xf>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};

		hrm_int: hrm-int {
			samsung,pins = "gpa0-6";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <3>;
		};
	};

	wacom_fwe_en {
		compatible = "regulator-fixed";
		regulator-name = "wacom_fwe_1.8v";
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		gpio = <&gpf5 1 0>;
		enable-active-high;
	};
	wacom_ldo_en {
		compatible = "regulator-fixed";
		regulator-name = "wacom_3.3v";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		gpio = <&gpf1 6 0>;
		enable-active-high;
		regulator-boot-on;
	};

	pinctrl@14CC0000 {
		epen_gpio: epen-gpio {
			samsung,pins = "gpd0-3", "gpd0-2";
			samsung,pin-function = <0>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
	};

	hsi2c@14E60000 {
		clock-frequency = <400000>;
		status = "okay";
		samsung,scl-clk-stretching;
		samsung,transfer_delay = <50>;
		pinctrl-names = "default", "epen_gpio";
		pinctrl-0 = <&hs_i2c2_bus &epen_ldo>;
		pinctrl-1 = <&epen_gpio>;
		wacom@56 {
			compatible = "wacom,w9010";
			reg = <0x56>;
			pinctrl-names = "on_irq", "off_state";
			pinctrl-0 = <&epen_irq &epen_sense>;
			pinctrl-1 = <&epen_input>;
			interrupts = <3 0 0>;
			interrupt-parent = <&gpf4>;
			/* gpios */
			wacom,irq-gpio = <&gpf4 3 0>;
			wacom,pdct-gpio = <&gpf4 4 0>;
			wacom,fwe-gpio = <&gpf5 1 0>;
			wacom,sense-gpio = <&gpa1 6 0>;
			wacom,irq_type = <2>;	/*IRQF_TRIGGER_FALLING*/
			/* features */
			wacom,max_coords = <12544 7056>;
			wacom,origin = <0 0>;
			wacom,max_pressure = <2047>;
			wacom,project_name = "TRE", "N910";
			wacom,invert = <1 0 1>; /* x y switch */
			wacom,fw_path = "epen/W9012_T.bin";
			wacom,ic_type = <9012>;
			wacom,boot_on_ldo = <1>;
		};
	};
	i2c@09 {
		compatible = "i2c-gpio";
		gpios = <&gpd0 2 0 &gpd0 3 0>;
		i2c-gpio,delay-us = <2>;
		#address-cells = <1>;
		#size-cells = <0>;
		status = "disabled";
		wacom@09 {
			compatible = "wacom,flash";
			reg = <0x09>;
			interrupts = <3 0 0>;
			interrupt-parent = <&gpf4>;
		};
	};

	hsi2c@14ED0000 {
		status = "okay";
		touchscreen@20 {
			stm,grip_area = <160>;
		};
	};

	ion {
		compatible = "samsung,exynos5430-ion";

		contig-names = "common",
			       "mfc_sh",
			       "g2d_wfd",
			       "video",
			       "sectbl",
			       "mfc_fw",
			       "mfc_nfw",
			       "secdma";

		contig = <0 0x600000 0x0>,
		         <2 0x100000 0x0>,
		         <10 0x800000 0x0>,
		         <11 0xA800000 0x0>,
		         <9 0x100000 0x0>,
		         <8 0x100000 0x0>,
		         <12 0x100000 0x0>,
		         <13 0x20000 0x0>;

		contig-isolate_on_boot = <0>, <2>, <8>,
					  <9>, <10>, <11>, <12>, <13>;

		ion_noncontig_heap {
			id-type = <0 0>;
		};

		exynos_contig_heap {
			id-type = <4 6>;
		};
	};

	pinctrl@10580000 {
		disp_pcd: disp_pcd {
