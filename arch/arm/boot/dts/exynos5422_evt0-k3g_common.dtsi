/* can not be bigger than period */
		haptic,duty = <32047>;
/* input freq : 233Hz */
		haptic,period = <33500>;
		haptic,reg2 = <0x82>;
		haptic,pwm_id = <0>;
		haptic,regulator_name = "vcc_3.0v_motor";
	};

	torch {
		status = "okay";
		leds1 {
			ledname = "leds-sec1";
