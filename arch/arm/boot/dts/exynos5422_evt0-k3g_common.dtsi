	};

	gpio_keys {
		compatible = "gpio-keys";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <100>;
		button@1 {
			label = "gpio-keys: KEY_POWER";
			interrupts = <2 0 0>;
			interrupt-parent = <&gpx2>;
			linux,code = <116>;
			gpios = <&gpx2 2 0xf>;
			gpio-key,wakeup = <1>;
		};
		button@2 {
			label = "gpio-keys: KEY_VOLUMEDOWN";
			interrupts = <3 0 0>;
			interrupt-parent = <&gpx0>;
			linux,code = <114>;
			gpios = <&gpx0 3 0xf>;
		};
		button@3 {
			label = "gpio-keys: KEY_VOLUMEUP";
			interrupts = <2 0 0>;
			interrupt-parent = <&gpx0>;
			linux,code = <115>;
			gpios = <&gpx0 2 0xf>;
		};
		button@4 {
			label = "gpio-keys: KEY_HOMEPAGE";
			interrupts = <5 0 0>;
			interrupt-parent = <&gpx0>;
			linux,code = <172>;
			gpios = <&gpx0 5 0xf>;
			gpio-key,wakeup = <1>;
		};
	};

